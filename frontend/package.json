        "element-plus": "^2.1.3",
        "highlight.js": "^11.5.0",
        "lodash-es": "^4.17.21",
        "normalize.css": "^8.0.1",
        "showdown": "^2.0.3",
        "vant": "^3.2.8",
        "vue": "^3.2.16",
        "vue-i18n": "^9.2.0-beta.23",
        "vue-router": "^4.0.0-0",
        "vuex": "^4.0.1"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.16.5",
        "@babel/eslint-plugin": "^7.16.5",
