        "lint-staged": "^12.1.2",
        "prettier": "^2.5.1",
        "prettier-eslint": "^13.0.0",
        "rollup-plugin-visualizer": "^5.5.2",
        "sass": "^1.45.0",
        "sass-loader": "^12.4.0",
        "typescript": "^4.4.4",
        "unplugin-vue-components": "^0.17.2",
        "vite": "^2.7.3",
